<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
  
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Home</servlet-class>
  </servlet>	  
  <servlet>
  	<servlet-name>ByDepartment</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Departments</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AllStations</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Stations</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Sector</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Sector</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Search</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Search</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ByCoordinates</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Coordinates</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Station</servlet-name>
  	<servlet-class>com.web.semantics.controllers.Station</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>Main</servlet-name>
  	<servlet-class>com.web.semantics.main.Main</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ByDepartment</servlet-name>
  	<url-pattern>/department</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AllStations</servlet-name>
  	<url-pattern>/stations</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Sector</servlet-name>
  	<url-pattern>/sector</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Search</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ByCoordinates</servlet-name>
  	<url-pattern>/coordinates</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Station</servlet-name>
  	<url-pattern>/station</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>Main</servlet-name>
  	<url-pattern>/main</url-pattern>
  </servlet-mapping>
  
  
</web-app>
